{
    "name": "slakbal/oauth",
    "description": "OAuth2 Authentication Service Provider",
    "keywords": [
        "oauth"
    ],
    "homepage": "https://github.com/slakbal/oauth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Leslie Price",
            "email": "leslie.price78@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "~9",
        "laravel/socialite": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.3",
        "orchestra/testbench": "^7.18",
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Slakbal\\Oauth\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Slakbal\\Oauth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-coverage": "./vendor/bin/phpunit --coverage-html coverage",
        "test": "./vendor/bin/pest",
        "fix": "./vendor/bin/pint",
        "check": "./vendor/bin/pint --test -v",
        "static": "./vendor/bin/phpstan analyse",
        "all": [
            "@test",
            "@fix",
            "@check",
            "@static"
        ]
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slakbal\\Oauth\\OauthServiceProvider"
            ],
            "aliases": {
                "Auth": "Slakbal\\Oauth\\Facades\\Oauth"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}